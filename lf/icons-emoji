# file types (with matching order)
ln      ï’       # LINK
or      ï’       # ORPHAN
tw      t       # STICKY_OTHER_WRITABLE
ow      ï“       # OTHER_WRITABLE
st      t       # STICKY
di      ï“       # DIR
pi      p       # FIFO
so      s       # SOCK
bd      b       # BLK
cd      c       # CHR
su      u       # SETUID
sg      g       # SETGID
ex      ï§       # EXEC
fi      ï       # FILE


