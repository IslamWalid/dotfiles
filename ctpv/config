preview media audio/* video/* {{
    exiftool -s \
    -filename \
    -directory \
    -filesize \
    -MIMEType \
    -filetype \
    -Duration \
    -VideoCodecID \
    -DocType \
    -ImageHeight \
    -ImageWidth \
    -DisplayHeight \
    -DisplayWidth \
    "${f}"
}}

preview iso .iso {{
    iso-info "${f}"
}}
